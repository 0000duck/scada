  <Dictionary key="Scada.Comm.Channels.FrmCommTcpClientProps">
    <Phrase key="this">TCP Client Properties</Phrase>
    <Phrase key="gbMode">Operating Mode</Phrase>
    <Phrase key="lblBehavior">Behavior</Phrase>
    <Phrase key="pbBehaviorHint.ToolTip">Master - Communicator sends request to a device and receives answer.&#13;&#10;Slave - Communicator passively waits for data from a device.</Phrase>
